 
#include<bits/stdc++.h>
using namespace std;
#define ll long long int  

int main() {
    ll t;
    cin>>t;
    
    while(t--){
        
        ll n;cin>>n;
        ll sum_of_squares = (n*(n+1)*(2*n+1))/6;
        ll square_of_sum  = (n*(n+1))/2*(n*(n+1))/2;
        
        cout<<square_of_sum-sum_of_squares<<'\n';
    }
    
    return 0;
 }
